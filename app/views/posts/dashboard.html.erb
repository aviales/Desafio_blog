<div class="container">
  <h1>Crear Post/Dashboard</h1>
  <form id="create_posts" method="get" action="/posts/create">
    <h2>Titulo</h2>
    <input name="title">
    <h2>Imagen</h2>
    <input name="image_url">
    <h2>Contenido</h2>
    <textarea name="content"></textarea>
    <input type="submit" value="Enviar">
  </form>
  <li><%= link_to "Volver a inicio", posts_index_path%></li>
</div>

